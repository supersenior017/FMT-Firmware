    "version": "0.2.0",
    "configurations": [
        {
            "name": "FMUv5 Debug J-Link",
            "type": "cortex-debug",
            "request": "launch",
            "cwd": "${workspaceRoot}",
            "executable": "target/pixhawk/fmu-v5/build/fmt_fmuv5.elf",
            "serverpath": "${env:JLINK_SERVER}",
            "servertype": "jlink",
            "device": "STM32F765II",
            "interface": "swd",
            "armToolchainPath": "${env:RTT_EXEC_PATH}",
            "serialNumber": "", //If you have more than one J-Link probe, add the serial number here.
            "svdFile": "target/pixhawk/fmu-v5/STM32F7x5.svd",
            "runToMain": true
        },
        {
            "name": "CUAV V5+ Debug J-Link",
            "type": "cortex-debug",
            "request": "launch",
            "cwd": "${workspaceRoot}",
            "executable": "target/cuav/v5_plus/build/fmt_fmuv5.elf",
            "serverpath": "${env:JLINK_SERVER}",
            "servertype": "jlink",
            "device": "STM32F765II",
            "interface": "swd",
            "armToolchainPath": "${env:RTT_EXEC_PATH}",
            "serialNumber": "", //If you have more than one J-Link probe, add the serial number here.
            "svdFile": "target/cuav/v5_plus/STM32F7x5.svd",
            "runToMain": true
        },
        {
            "name": "FMUv2 Debug J-Link",
            "type": "cortex-debug",
            "request": "launch",
            "cwd": "${workspaceRoot}",
            "executable": "target/pixhawk/fmu-v2/build/fmt_fmuv2.elf",
            "serverpath": "${env:JLINK_SERVER}",
            "servertype": "jlink",
            "device": "STM32F427VI",
            "interface": "swd",
            "armToolchainPath": "${env:RTT_EXEC_PATH}",
            "serialNumber": "", //If you have more than one J-Link probe, add the serial number here.
            "svdFile": "target/pixhawk/fmu-v2/STM32F427.svd",
            "runToMain": true
        }
    ]
}