from building import *

cwd = GetCurrentDir()

src = []
inc = []

src += Glob('comm/*.c')
inc += [cwd + '/comm']

src += Glob('fmtio/*.c')
inc += [cwd + '/fmtio']

src += Glob('logger/*.c')
inc += [cwd + '/logger']

src += Glob('status/*.c')
inc += [cwd + '/status']

src += Glob('vehicle/multicopter/*.c')
inc += [cwd + '/vehicle/multicopter']

group = DefineGroup('Task', src, depend = [''], CPPPATH = inc)

Return('group')

